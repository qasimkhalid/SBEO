@prefix : <http://www.ia.urjc.es/ontologies/building/sbeo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.ia.urjc.es/ontologies/building/sbeo> .

<http://www.ia.urjc.es/ontologies/building/sbeo> rdf:type owl:Ontology ;
                                                  <http://purl.org/dc/elements/1.1/rights> <https://www.gnu.org/licenses/gpl.html> ;
                                                  <http://purl.org/dc/terms/creator> "Qasim Khalid" ;
                                                  <http://purl.org/dc/terms/description> """Smart Building Evacuation Ontology (SBEO) is an ontology that couples the information about any building with its occupants such that it can be used in many useful ways. For example, indoor localization of people, detection of any hazard, a recommendation of normal routes such as shopping or stadium seating routes, or safe and feasible emergency evacuation routes or both of them all together.  

The core SBEO covers the concepts related to the geometry of building, devices and components of the building, route graphs correspondent to the building topology, users’ characteristics and preferences, situational awareness of both building (hazard detection, status of routes in terms of availability and occupancy) and users (tracking, management of groups, status in terms of fitness), and emergency evacuation."""@en ;
                                                  <http://purl.org/dc/terms/language> "en" ;
                                                  <http://purl.org/dc/terms/title> "SBEO: Smart Building Evacuation Ontology"@en ;
                                                  rdfs:seeAlso <https://github.com/qasimkhalid/SBEO> ;
                                                  owl:versionInfo 0.1 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/rights
<http://purl.org/dc/elements/1.1/rights> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/language
<http://purl.org/dc/terms/language> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/ontology/olo/core#item
<http://purl.org/ontology/olo/core#item> rdf:type owl:ObjectProperty ;
                                         rdfs:domain <http://purl.org/ontology/olo/core#Slot> ;
                                         rdfs:comment "An item of a slot in an ordered list."@en ;
                                         rdfs:label "has item"@en .


###  http://purl.org/ontology/olo/core#next
<http://purl.org/ontology/olo/core#next> rdf:type owl:ObjectProperty ;
                                         owl:inverseOf <http://purl.org/ontology/olo/core#previous> ;
                                         rdf:type owl:FunctionalProperty ;
                                         rdfs:domain <http://purl.org/ontology/olo/core#Slot> ;
                                         rdfs:range <http://purl.org/ontology/olo/core#Slot> ;
                                         rdfs:comment "Associates the next slot in an ordered list."@en ;
                                         rdfs:isDefinedBy <http://purl.org/ontology/olo/core#> ;
                                         rdfs:label "has next"@en .


###  http://purl.org/ontology/olo/core#ordered_list
<http://purl.org/ontology/olo/core#ordered_list> rdf:type owl:ObjectProperty ;
                                                 owl:inverseOf <http://purl.org/ontology/olo/core#slot> ;
                                                 rdf:type owl:FunctionalProperty ;
                                                 rdfs:domain <http://purl.org/ontology/olo/core#Slot> ;
                                                 rdfs:range <http://purl.org/ontology/olo/core#OrderedList> ;
                                                 rdfs:comment "An ordered list of an slot."@en ;
                                                 rdfs:isDefinedBy <http://purl.org/ontology/olo/core#> ;
                                                 rdfs:label "has ordered list"@en .


###  http://purl.org/ontology/olo/core#previous
<http://purl.org/ontology/olo/core#previous> rdf:type owl:ObjectProperty ,
                                                      owl:InverseFunctionalProperty ;
                                             rdfs:domain <http://purl.org/ontology/olo/core#Slot> ;
                                             rdfs:range <http://purl.org/ontology/olo/core#Slot> ;
                                             rdfs:comment "Associates the previous slot in an ordered list"@en ;
                                             rdfs:isDefinedBy <http://purl.org/ontology/olo/core#> ;
                                             rdfs:label "has previous"@en .


###  http://purl.org/ontology/olo/core#slot
<http://purl.org/ontology/olo/core#slot> rdf:type owl:ObjectProperty ;
                                         rdfs:domain <http://purl.org/ontology/olo/core#OrderedList> ;
                                         rdfs:range <http://purl.org/ontology/olo/core#Slot> ;
                                         rdfs:comment "A slot in an ordered list."@en ;
                                         rdfs:isDefinedBy <http://purl.org/ontology/olo/core#> ;
                                         rdfs:label "has slot"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#accompanying
:accompanying rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
              rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
              rdfs:comment "Be present or associated with a social unit."@en ;
              rdfs:label "Accompanying"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#acquaintanceOf
:acquaintanceOf rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                rdfs:comment "Any friend or an acquaintance tie among occupants."@en ;
                rdfs:label "Acquaintance Of"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#adjacentTo
:adjacentTo rdf:type owl:ObjectProperty ,
                     owl:SymmetricProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :RouteElement
                                        :Space
                                      )
                        ] ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :RouteElement
                                       :Space
                                     )
                       ] ;
            rdfs:comment "Nearest in space or position; immediately adjoining without intervening space or element; an exact neighbour."@en ;
            rdfs:label "Adjacent To"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#assignedRoute
:assignedRoute rdf:type owl:ObjectProperty ;
               rdfs:domain :SocialUnit ;
               rdfs:range :Route ;
               rdfs:comment "A set of connected spaces(normally in the form of rdf:list) assigned to a social unit."@en ;
               rdfs:label "Assigned Route"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#connectedTo
:connectedTo rdf:type owl:ObjectProperty ,
                      owl:SymmetricProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :RouteElement
                                         :Space
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :RouteElement
                                        :Space
                                      )
                        ] ;
             rdfs:comment "Being joined by a common element; more specificially, a physical connection between two space elements."@en ;
             rdfs:label "Connected To"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#dependentOn
:dependentOn rdf:type owl:ObjectProperty ;
             owl:inverseOf :responsibleTo ;
             rdf:type owl:TransitiveProperty ;
             rdfs:domain [ owl:intersectionOf ( <http://xmlns.com/foaf/0.1/Person>
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasRole ;
                                                  owl:hasValue :Dependent
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
             rdfs:range [ owl:intersectionOf ( <http://xmlns.com/foaf/0.1/Person>
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :hasRole ;
                                                 owl:hasValue :Responsible
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
             rdfs:comment "Requiring someone to perform a specific activity."@en ;
             rdfs:label "Dependent On"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#excludedFor
:excludedFor rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :PointOfInterest
                                         :RouteElement
                                         :Space
                                       )
                         ] ;
             rdfs:range :SocialUnit ;
             rdfs:comment "Expresses the removal of any physical space or point of interest for the consideration of any social unit."@en ;
             rdfs:label "Excluded For"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#familiarWith
:familiarWith rdf:type owl:ObjectProperty ;
              rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :PointOfInterest
                                         :Route
                                         :RouteElement
                                         :Space
                                       )
                         ] ;
              rdfs:comment "States the familiarity of any social unit with a physical space or point of interest."@en ;
              rdfs:label "Familiar With"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasActivityStatus
:hasActivityStatus rdf:type owl:ObjectProperty ;
                   rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:range :ActivityStatus ;
                   rdfs:comment "The status of an activity being performed by a person."@en ;
                   rdfs:label "Has Activity Status"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasAvailabilityStatus
:hasAvailabilityStatus rdf:type owl:ObjectProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :Device
                                                   :Route
                                                   :Space
                                                 )
                                   ] ;
                       rdfs:range :AvailabilityStatus ;
                       rdfs:comment "The status of something in terms of availability. For example, available or unavailable."@en ;
                       rdfs:label "Has Availability Status"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasDescription
:hasDescription rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Activity
                                            :Device
                                            :Event
                                            :Route
                                            :RouteElement
                                            :Space
                                          )
                            ] ;
                rdfs:range :Description ;
                rdfs:comment "Expresses a descriptive information about any specific space, point, activiy, event or device."@en ;
                rdfs:label "Has Description"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasDeviationState
:hasDeviationState rdf:type owl:ObjectProperty ;
                   rdfs:comment "A deviation status of a person depending on how ofthen one deviates from the proposed route."@en ;
                   rdfs:label "Has Deviation State"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasFitnessStatus
:hasFitnessStatus rdf:type owl:ObjectProperty ;
                  rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                  rdfs:range :FitnessStatus ;
                  rdfs:comment "A fitness status is associated with a person that represent the quality of being suitable for a specific activity. There are three different levels of fitness levels are introduced that can be extended according to the needs of the application. These are 'Fit', 'Exhausted' and 'Injured'."@en ;
                  rdfs:label "Has Fitness Status"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           rdfs:domain :Group ;
           rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
           rdfs:comment "Expresses one of the persons who compose a social group."@en ;
           rdfs:label "Has Member"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasMotionState
:hasMotionState rdf:type owl:ObjectProperty ;
                rdfs:domain :SocialUnit ;
                rdfs:range :MotionState ;
                rdfs:comment "The motion state of a person or a device."@en ;
                rdfs:label "Has Motion State"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasNavigationalState
:hasNavigationalState rdf:type owl:ObjectProperty ;
                      rdfs:domain :SocialUnit ;
                      rdfs:range :NavigationalState ;
                      rdfs:comment "A navigational state of a person while following any path."@en ;
                      rdfs:label "Has Navigational State"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasNavigationalType
:hasNavigationalType rdf:type owl:ObjectProperty ;
                     rdfs:domain :SocialUnit ;
                     rdfs:range :Navigation ;
                     rdfs:comment "A type of navigational provided to a social unit to perform a specific activity."@en ;
                     rdfs:label "Has Navigation Type"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
         rdfs:range :Role ;
         rdfs:comment "Expresses a relation between a person and a role."@en ;
         rdfs:label "Has Role"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#installedIn
:installedIn rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Device
                                         :PointOfInterest
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :PointOfInterest
                                        :Route
                                        :RouteElement
                                        :Space
                                      )
                        ] ;
             rdfs:comment "Place or fix (equipment or machinery) in position ready for use (Oxford Dictionary of English)."@en ;
             rdfs:label "Installed In"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#leadsTo
:leadsTo rdf:type owl:ObjectProperty ,
                  owl:SymmetricProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :RouteElement
                                     :Space
                                   )
                     ] ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :PointOfInterest
                                    :RouteElement
                                    :Space
                                  )
                    ] ;
         rdfs:comment "Be a means of access to a particular space or point."@en ;
         rdfs:label "Lead To"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#locatedIn
:locatedIn rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Activity
                                       :Event
                                       :PointOfInterest
                                       :RouteElement
                                       :SocialUnit
                                       :Space
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Route
                                      :RouteElement
                                      :Space
                                    )
                      ] ;
           rdfs:comment "States the location of any social unit, activity, event, physical space, or a point of interest at any other physical space."@en ;
           rdfs:label "Located In"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#meansOfNotification
:meansOfNotification rdf:type owl:ObjectProperty ;
                     rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( :AlertingDevice
                                                :DisplayScreen
                                                :HandheldDevice
                                                :Telephone
                                              )
                                ] ;
                     rdfs:comment "A method of notifying a social unit."@en ;
                     rdfs:label "Means of Notification"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#notAccessibleTo
:notAccessibleTo rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :PointOfInterest
                                             :RouteElement
                                             :Space
                                           )
                             ] ;
                 rdfs:range :SocialUnit ;
                 rdfs:comment "Expresses the prohibition of access of any specific foaf:Person to a specific physical space."@en ;
                 rdfs:label "Not Accessible To"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#partOf
:partOf rdf:type owl:ObjectProperty ,
                 owl:TransitiveProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :RouteElement
                                    :Space
                                  )
                    ] ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :RouteElement
                                   :Space
                                 )
                   ] ;
        rdfs:comment "Expresses a physical space as a part or a fragment of another physical space. For example, a block of apartments can be a part of apartment building, etc."@en ;
        rdfs:label "Part Of"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#performedBy
:performedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Activity ;
             rdfs:range :SocialUnit ;
             rdfs:comment "Expresses an activity carried out by a social unit."@en ;
             rdfs:label "Performed by"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#responsibleTo
:responsibleTo rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain [ owl:intersectionOf ( <http://xmlns.com/foaf/0.1/Person>
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasRole ;
                                                    owl:hasValue :Responsible
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
               rdfs:range [ owl:intersectionOf ( <http://xmlns.com/foaf/0.1/Person>
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasRole ;
                                                   owl:hasValue :Dependent
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "Having an obligation to do something, or having control over or care for someone, as part of one's role (Oxford Dictionary of English)."@en ;
               rdfs:label "Responsible To"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#routePreference
:routePreference rdf:type owl:ObjectProperty ;
                 rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                 rdfs:range :GraphBasedRoute ;
                 rdfs:comment "Route preference of a social unit to perform an activity."@en ;
                 rdfs:label "Route Preference"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#routeType
:routeType rdf:type owl:ObjectProperty ;
           rdfs:domain :Route ;
           rdfs:range :GraphBasedRoute ;
           rdfs:comment "This property is used to express the type of a route in terms of graph-based routes, such as Shortest Path, Simplest Path etc."@en ;
           rdfs:label "Route Type"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#uses
:uses rdf:type owl:ObjectProperty ;
      rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :SocialUnit
                                  <http://xmlns.com/foaf/0.1/Person>
                                )
                  ] ;
      rdfs:range [ rdf:type owl:Class ;
                   owl:unionOf ( :Device
                                 :PointOfInterest
                                 :Route
                                 :RouteElement
                                 :Space
                               )
                 ] ;
      rdfs:comment "Expresses a momentary information about a physical space or a point being used by a social unit."@en ;
      rdfs:label "Uses"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/ontology/olo/core#index
<http://purl.org/ontology/olo/core#index> rdf:type owl:DatatypeProperty ,
                                                   owl:FunctionalProperty ;
                                          rdfs:domain <http://purl.org/ontology/olo/core#Slot> ;
                                          rdfs:range xsd:positiveInteger ;
                                          rdfs:comment "An index of a slot in an ordered list."@en ;
                                          rdfs:isDefinedBy <http://purl.org/ontology/olo/core#> ;
                                          rdfs:label "has index"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#ID
:ID rdf:type owl:DatatypeProperty ;
    rdfs:domain [ rdf:type owl:Class ;
                  owl:unionOf ( :Device
                                :SocialUnit
                              )
                ] ;
    rdfs:range xsd:integer ;
    rdfs:comment "An unique identification number of something such as, social unit, device, etc."@en ;
    rdfs:label "Identification Number"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#accommodationCapacity
:accommodationCapacity rdf:type owl:DatatypeProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :PointOfInterest
                                                   :RouteElement
                                                   :Space
                                                 )
                                   ] ;
                       rdfs:range xsd:integer ;
                       rdfs:comment "The accommodating capacity (in terms of persons) of a particular space."@en ;
                       rdfs:label "Accommodation Capacity"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#atTime
:atTime rdf:type owl:DatatypeProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Activity
                                    :Device
                                    :Event
                                    :SocialUnit
                                  )
                    ] ;
        rdfs:comment "The time at which an activity or event occurred."@en ;
        rdfs:label "At Time"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#cost
:cost rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdfs:domain :Route ,
                  :RouteElement ;
      rdfs:comment "A numeric value associated with an edge or a complete route(path). It is also reffered to as a weight."@en ;
      rdfs:label "Cost"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#currentOccupancy
:currentOccupancy rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :PointOfInterest
                                              :Route
                                              :RouteElement
                                              :Space
                                            )
                              ] ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "The current occupancy (in terms of persons) of a particular space."@en ;
                  rdfs:label "Current Occupancy"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#dynamicDescription
:dynamicDescription rdf:type owl:DatatypeProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( :Activity
                                                :Event
                                                :PointOfInterest
                                                :SocialUnit
                                                :Space
                                              )
                                ] ;
                    rdfs:range xsd:string ;
                    rdfs:comment "A dynamic or momentary information about a specific activity, event, social unit, or a space."@en ;
                    rdfs:label "Dynamic Description"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#endedAtTime
:endedAtTime rdf:type owl:DatatypeProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Activity
                                         :Event
                                       )
                         ] ;
             owl:propertyDisjointWith :startedAtTime ;
             rdfs:comment "The time at which an activity ended."@en ;
             rdfs:label "Ended At Time"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasSmoke
:hasSmoke rdf:type owl:DatatypeProperty ;
          rdfs:domain :SmokeSensor ;
          rdfs:range xsd:boolean ;
          rdfs:comment "It is used to describe if a smoke sensor detects smoke. It is expressed as Boolean in which true means the smoke is detected and the false means there is no smoke detected by the smoke sensor."@en ;
          rdfs:label "Has Smoke"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasXTimesDeviated
:hasXTimesDeviated rdf:type owl:DatatypeProperty ;
                   rdfs:domain :SocialUnit ;
                   rdfs:range xsd:integer ;
                   rdfs:comment "Number of times a social unit is deviated while following the provided path."@en ;
                   rdfs:label "Has X Time Deviated"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#length
:length rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( <http://purl.org/ontology/olo/core#OrderedList>
                                    :Passage
                                  )
                    ] ;
        rdfs:range xsd:nonNegativeInteger ;
        rdfs:comment "The measurement or extent of something from end to end (Oxford Dictionary of English)."@en ;
        rdfs:label "Length"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#speed
:speed rdf:type owl:DatatypeProperty ;
       rdfs:domain :SocialUnit ;
       rdfs:comment "The average speed of a person."@en ;
       rdfs:label "Speed"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#speedFactor
:speedFactor rdf:type owl:DatatypeProperty ;
             rdfs:domain :RouteElement ;
             rdfs:range xsd:decimal ;
             rdfs:comment "A multiplying factor that may affect the speed of person while passing through a specific space."@en ;
             rdfs:label "Speed Factor"@en .


###  http://www.ia.urjc.es/ontologies/building/sbeo#startedAtTime
:startedAtTime rdf:type owl:DatatypeProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Activity
                                           :Event
                                         )
