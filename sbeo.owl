#################################################################
#    General axioms
#################################################################

[ rdf:type owl:Restriction ;
  owl:onProperty sbeo:hasMember ;
  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
  owl:onClass [ owl:intersectionOf ( foaf:Person
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty sbeo:role ;
                                       owl:hasValue sbeo:Leader
                                     ]
                                   ) ;
                rdf:type owl:Class
              ] ;
  rdfs:subClassOf sbeo:EmergencyEvacuationGroup
] .


[ rdf:type owl:Restriction ;
  owl:onProperty sbeo:hasMember ;
  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
  owl:onClass [ owl:intersectionOf ( foaf:Person
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty sbeo:role ;
                                       owl:hasValue sbeo:Responsible
                                     ]
                                   ) ;
                rdf:type owl:Class
              ] ;
  rdfs:subClassOf sbeo:Family
] .


