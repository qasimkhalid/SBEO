               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( sbeo:Activity
                                           sbeo:Event
                                           sbeo:PointOfInterest
                                           sbeo:RouteElement
                                           sbeo:SocialUnit
                                           sbeo:Space
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( sbeo:Route
