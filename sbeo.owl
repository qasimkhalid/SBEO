@prefix sbeo: <http://www.ia.urjc.es/ontologies/building/sbeo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix seas: <https://w3id.org/seas/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://www.ia.urjc.es/ontologies/building/sbeo> .

<http://www.ia.urjc.es/ontologies/building/sbeo> rdf:type owl:Ontology ;
                                                  owl:versionIRI <http://www.ia.urjc.es/ontologies/building/sbeo/2.0> ;
                                                  owl:imports <https://raw.githubusercontent.com/qasimkhalid/SBEO/master/imports/BuildingOntology-1.0.owl> ;
                                                  dcterms:creator "Qasim Khalid" ;
                                                  dcterms:description """Smart Building Evacuation Ontology (SBEO) is an ontology that couples the information about any building with its occupants such that it can be used in many useful ways. For example, indoor localization of people, detection of any hazard, a recommendation of shopping or stadium seating routes, a recommendation of safe and feasible emergency evacuation routes, or all together. 

The core SBEO uses the three major concepts: topology of the building, situation awareness in the building, and the information about the physical characteristics and preferences of each person located in the building. 

Following are some general concepts in SBEO: Person, BuildingSpace, BuildingSpaceConnection, Incident, Device, SpaceCharacteristic, Route and Distance. As the reusing the vocabulary is the essence of linked data and semantic web, therefore in SBEO, we have also reused three external vocabularies, i.e. Person from FOAF(http://xmlns.com/foaf/spec/), and both BuildingSpace and BuildingSpaceConnection from SEAS Building Ontology (https://ci.mines-stetienne.fr/seas/BuildingOntology-1.0). On the other hand, we have introduced some classes in SBEO which are the subclasses of these existing vocabularies."""^^xsd:string ;
                                                  dcterms:title "SBEO: Smart Building Evacuation Ontology" ;
                                                  owl:versionInfo 2.0 .

#################################################################
#    Object Properties
#################################################################

###  http://www.ia.urjc.es/ontologies/building/sbeo#accompanying
sbeo:accompanying rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ,
                           owl:TransitiveProperty ;
                  rdfs:domain foaf:Person ;
                  rdfs:range foaf:Person ;
                  rdfs:comment "This property is used to mention how who is accompanying who in a common seas:BuildingSpace."^^xsd:string ;
                  rdfs:label "accompanying"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#acquaintanceOf
sbeo:acquaintanceOf rdf:type owl:ObjectProperty ,
                             owl:SymmetricProperty ;
                    rdfs:domain foaf:Person ;
                    rdfs:range foaf:Person ;
                    rdfs:comment "This property is used to represent any friend or acquaintace tie among occupants."^^xsd:string ;
                    rdfs:label "acquaintanceOf"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#adjacentTo
sbeo:adjacentTo rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain seas:Zone ;
                rdfs:range seas:Zone ;
                rdfs:comment "This property is used to mention the spaces that are adjacent to a specific space by a wall. This property is helpful for security purposes, for example, in case of any emergency situation when the seas:BuildingSpaceConnection, i.e. door, windows, are not accessible."^^xsd:string ;
                rdfs:label "adjacentTo"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#assignedRoute
sbeo:assignedRoute rdf:type owl:ObjectProperty ;
                   rdfs:domain foaf:Person ;
                   rdfs:range sbeo:Route ;
                   rdfs:comment "This property is used to represent a route (rdf:list of connected spaces) that is assigned to any  person or a group of persons." ;
                   rdfs:label "assignedRoute" .


###  http://www.ia.urjc.es/ontologies/building/sbeo#connectedTo
sbeo:connectedTo rdf:type owl:ObjectProperty ,
                          owl:SymmetricProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( sbeo:Entrance
                                             sbeo:Exit
                                             seas:Zone
                                             seas:ZoneConnection
                                           )
                             ] ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( sbeo:Entrance
                                            sbeo:Exit
                                            seas:Zone
                                            seas:ZoneConnection
                                          )
                            ] ;
                 rdfs:comment "This property is used to mention the connectivity of any seas:BuildingSpaceConnection or seas:BuildingSpace with each other."^^xsd:string ;
                 rdfs:label "conectedTo"^^xsd:string ;
                 rdfs:seeAlso <seas:connectedTo> .


###  http://www.ia.urjc.es/ontologies/building/sbeo#dependentOn
sbeo:dependentOn rdf:type owl:ObjectProperty ;
                 owl:inverseOf sbeo:responsibleTo ;
                 rdf:type owl:TransitiveProperty ;
                 rdfs:domain foaf:Person ;
                 rdfs:range foaf:Person ;
                 rdfs:comment "This property is used to mention the dependency of a particular person on his/her responsible person. When this property is used as a predicate in any triple, the object of that triple has the IRI of the instance of responsible person who has the responsibility of this dependent person."^^xsd:string ;
                 rdfs:label "dependentOn"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#distanceOf
sbeo:distanceOf rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain sbeo:Distance ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( sbeo:Entrance
                                           sbeo:Exit
                                           seas:Zone
                                           seas:ZoneConnection
                                         )
                           ] ;
                rdfs:comment "This property is used to represent the distance between any two connected seas:BuildingSpaceConnection or seas:BuildingSpace."^^xsd:string ;
                rdfs:label "distanceOf"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#endsIn
sbeo:endsIn rdf:type owl:ObjectProperty ;
            rdfs:domain sbeo:Route ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( sbeo:Entrance
                                       sbeo:Exit
                                       sbeo:Route
                                       seas:Zone
                                       seas:ZoneConnection
                                     )
                       ] ;
            rdfs:comment "This property is used to express the last space where the route ends. Generally, these are sbeo:Exit."^^xsd:string ;
            rdfs:label "endsIn"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasPath
sbeo:hasPath rdf:type owl:ObjectProperty ;
             rdfs:domain sbeo:Route ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( seas:Zone
                                        seas:ZoneConnection
                                      )
                        ] ;
             rdfs:comment "This property is used to express the set of connected building spaces or building space connections that makes a route. In this regard, when this property is used as a predicate in a triple, the object of that triple must be a rdf:list."^^xsd:string ;
             rdfs:label "hasPath"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#installedIn
sbeo:installedIn rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 rdfs:domain sbeo:Device ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( seas:Zone
                                            seas:ZoneConnection
                                          )
                            ] ;
                 rdfs:comment "This property is used to mention any device, system, or instrument, installed in a seas:BuildingSpace."^^xsd:string ;
                 rdfs:label "installedIn"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#locatedIn
sbeo:locatedIn rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( sbeo:Incident
                                           foaf:Person
                                           seas:Zone
                                           seas:ZoneConnection
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( seas:Zone
                                          seas:ZoneConnection
                                        )
                          ] ;
               rdfs:comment "This property is used to mention the existence of anything in a particular seas:BuildingSpace, such as person, or stairs, doors, etc."^^xsd:string ;
               rdfs:label "locatedIn"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#notAccessibleTo
sbeo:notAccessibleTo rdf:type owl:ObjectProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( seas:Zone
                                                 seas:ZoneConnection
                                               )
                                 ] ;
                     rdfs:range foaf:Person ;
                     rdfs:comment "This property is used to prohibit the access of any specific foaf:Person to a specific seas:BuildingSpace or seas:BuildingSpaceConnection."^^xsd:string ;
                     rdfs:label "notAccessibleTo"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#responsibleTo
sbeo:responsibleTo rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:domain foaf:Person ;
                   rdfs:range foaf:Person ;
                   rdfs:comment "This property is used to mention the responsibility for a particular person to his/her dependent persons. When this property is used as a predicate in any triple, the object of that triple has the IRI of the instance of dependent person associated with the subject of this responsible person."^^xsd:string ;
                   rdfs:label "responsibleTo"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#startsFrom
sbeo:startsFrom rdf:type owl:ObjectProperty ;
                rdfs:domain sbeo:Route ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( sbeo:Entrance
                                           sbeo:Exit
                                           sbeo:Route
                                           seas:Zone
                                           seas:ZoneConnection
                                         )
                           ] ;
                rdfs:comment "This property is used to express the space where any route starts."^^xsd:string ;
                rdfs:label "startsFrom"^^xsd:string .


#################################################################
#    Data properties
#################################################################

###  http://www.ia.urjc.es/ontologies/building/sbeo#accommodationCapacity
sbeo:accommodationCapacity rdf:type owl:DatatypeProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( sbeo:SafeZone
                                                       seas:Zone
                                                       seas:ZoneConnection
                                                     )
                                       ] ;
                           rdfs:range xsd:integer ;
                           rdfs:comment "This property is used to represent the accommodating capacity (in terms of persons) of a particular space."^^xsd:string ;
                           rdfs:label "accommodationCapacity"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#atTime
sbeo:atTime rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( sbeo:Incident
                                        sbeo:Sensor
                                        foaf:Person
                                      )
                        ] ;
            rdfs:range xsd:long ;
            rdfs:comment "It is used to represent a timestamp with any particular event. It is a calculated as Unix epoch. For example, when a specific person is located in a specific space, or a specific sensor detects a specific value, etc."^^xsd:string ;
            rdfs:label "atTime"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#currentOccupancy
sbeo:currentOccupancy rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( sbeo:SafeZone
                                                  seas:Zone
                                                  seas:ZoneConnection
                                                )
                                  ] ;
                      rdfs:range xsd:integer ;
                      rdfs:comment "This property is used to represent the current occupancy(in terms of persons) of a particular space."^^xsd:string ;
                      rdfs:label "currentOccupancy"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#evacuationStatus
sbeo:evacuationStatus rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf owl:topDataProperty ;
                      rdfs:domain foaf:Person ;
                      rdfs:range xsd:string ;
                      rdfs:comment "It represents the status of evacuation of each person. This property is specially used for evacuation purposes. Its value can be either Ã¢â‚¬Ëœnot startedÃ¢â‚¬â„¢ or Ã¢â‚¬Ëœin progressÃ¢â‚¬â„¢ or Ã¢â‚¬ËœcompletedÃ¢â‚¬â„¢. The value remains Ã¢â‚¬Ëœnot startedÃ¢â‚¬â„¢ in two cases. The first case, when there is no hazard and emergency evacuation has been started. The second case, when a hazard is detected and the emergency evacuation has not been started by person; both intentionally and unintentionally. The value of this property becomes Ã¢â‚¬Ëœin progressÃ¢â‚¬â„¢ when a person starts to move from oneÃ¢â‚¬â„¢s initial position and remains as it is until one has not arrived its assigned exit. Once a person arrives the exit, the evacuation status changes to Ã¢â‚¬ËœcompletedÃ¢â‚¬â„¢, that represents the evacuation process has been finished for that specific person."^^xsd:string ;
                      rdfs:label "evacuationStatus"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#familiarityWithBuilding
sbeo:familiarityWithBuilding rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf owl:topDataProperty ;
                             rdfs:domain foaf:Person ;
                             rdfs:range xsd:boolean ;
                             rdfs:comment "It represents whether a person has some level of familiarity with the building geometry or not. As its value is in Boolean, therefore, the true means that a person is familiar with the building whereas a false means (s)he is not."^^xsd:string ;
                             rdfs:label "familiarityWithBuilding"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#familyID
sbeo:familyID rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain foaf:Person ;
              rdfs:range xsd:integer ;
              rdfs:comment "It represents a unique identity number (ID) for persons having family ties. All the members of a family have one common familyID."^^xsd:string ;
              rdfs:label "familyID"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#fitnessLevel
sbeo:fitnessLevel rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain foaf:Person ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "This property is used to represent the fitness level of a person. For example, there are 5 levels. Upper three levels are appropriate and considered as safe whereas the below two levels are considered as dangerous and a particular person needs assistance. The person is considered as sbeo:Injured, if the levels are changed from upper three to lower two."^^xsd:string ;
                  rdfs:label "fitnessLevel"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#groupID
sbeo:groupID rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain foaf:Person ;
             rdfs:range xsd:integer ;
             rdfs:comment "This property is used to represent an identification number(ID) that is assigned to people who are evacuating together and having the same sbeo:assignedRoute."^^xsd:string ;
             rdfs:label "groupID"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasID
sbeo:hasID rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain foaf:Person ;
           rdfs:range xsd:integer ;
           rdfs:comment "It represents an unque identification number for each occupant of the building. This ID is used to calculate all the functionalities for a specific person, e.g. location, characteristics, preferences, etc"^^xsd:string ;
           rdfs:label "hasID"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasLength
sbeo:hasLength rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain sbeo:Route ;
               rdfs:range xsd:integer ;
               rdfs:comment "It represents the length of a route in terms of time(in seconds) or distance(in meters). More explanation of the route can be found in sbeo:Route class."^^xsd:string ;
               rdfs:label "hasLength"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasSmoke
sbeo:hasSmoke rdf:type owl:DatatypeProperty ;
              rdfs:domain sbeo:SmokeSensor ;
              rdfs:range xsd:boolean ;
              rdfs:label "This property is used to describe if a sbeo:SmokeSensor detects smoke or not. As its range is Boolean, therefore the true means the smoke is detected and the false means there is no smoke detected by a sbeo:SmokeSensor."^^xsd:string ,
                         "hasSmoke"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#hasValue
sbeo:hasValue rdf:type owl:DatatypeProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( sbeo:Device
                                                  seas:Distance
                                                )
                                  ] ;
              rdfs:range xsd:integer ;
              rdfs:comment "It represents the value of detected by any sbeo:Device."^^xsd:string ;
              rdfs:label "hasValue"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#needsSupervision
sbeo:needsSupervision rdf:type owl:DatatypeProperty ;
                      rdfs:domain foaf:Person ;
                      rdfs:range xsd:boolean ;
                      rdfs:comment "This property is used to mention either a person needs supervision or not. As its value is in Boolean, therefore, the true means that a person needs the supervision whereas a false means (s)he does not."^^xsd:string ;
                      rdfs:label "needsSupervision"^^xsd:string .


###  http://xmlns.com/foaf/0.1/age
foaf:age rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain foaf:Person ;
         rdfs:range xsd:integer ;
         rdfs:comment "The age of the person."^^xsd:string ;
         rdfs:label "age"^^xsd:string .


###  http://xmlns.com/foaf/0.1/firstName
foaf:firstName rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain foaf:Person ;
               rdfs:range xsd:string ;
               rdfs:comment "The first name of the person."^^xsd:string ;
               rdfs:label "firstName"^^xsd:string .


###  http://xmlns.com/foaf/0.1/lastName
foaf:lastName rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain foaf:Person ;
              rdfs:range xsd:string ;
              rdfs:comment "The last name of the person."^^xsd:string ;
              rdfs:label "lastName"^^xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.ia.urjc.es/ontologies/building/sbeo#ActiveFireProtection
sbeo:ActiveFireProtection rdf:type owl:Class ;
                          rdfs:subClassOf sbeo:FireProtection ;
                          rdfs:comment "ActiveFireProtection"^^xsd:string ,
                                       "It is an integral part of the fire protection used as a first response to minimize the spread of fire and related effects."^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#AssemblyPoint
sbeo:AssemblyPoint rdf:type owl:Class ;
                   rdfs:subClassOf sbeo:SafeZone ;
                   rdfs:comment "a place where people in an office, etc. should go if there is an emergency, for example, a fire (Cambridge English Dictionary) ."^^xsd:string ;
                   rdfs:label "AssemblyPoint"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Available
sbeo:Available rdf:type owl:Class ;
               rdfs:subClassOf sbeo:SpaceCharacteristic ;
               owl:disjointWith sbeo:Unavailable ;
               rdfs:comment "This is used to specify the availability of a parituclar space, route, etc. If its available, it means it can be accessible to foaf:Person subject to the characteristics of that person."^^xsd:string ;
               rdfs:label "Available"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#ChemicalSpill
sbeo:ChemicalSpill rdf:type owl:Class ;
                   rdfs:subClassOf sbeo:Incident ;
                   rdfs:comment "A hazardous event that happens due to the spillage or leakge of any toxic chemical in the building."^^xsd:string ;
                   rdfs:label "ChemicalSpill"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Congestion
sbeo:Congestion rdf:type owl:Class ;
                rdfs:subClassOf sbeo:Incident ;
                rdfs:comment "An event that happens at bottlenecks when the flow of people increases than a provided limit due to herding (and similar phenomena of human behaviour), during an emergency evacuation process. As a result, overall escape time is increased."^^xsd:string ;
                rdfs:label "Congestion"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#DayCare
sbeo:DayCare rdf:type owl:Class ;
             rdfs:subClassOf seas:Room ;
             rdfs:comment "A space for childcare supervised by a nanny while parents are at work."^^xsd:string ;
             rdfs:label "DayCare"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Dependent
sbeo:Dependent rdf:type owl:Class ;
               rdfs:subClassOf foaf:Person ;
               owl:disjointWith sbeo:Responsible ;
               rdfs:comment "It represents the dependency of a person on other persons in case of any emergency condition."^^xsd:string ;
               rdfs:label "Dependent"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Device
sbeo:Device rdf:type owl:Class ;
            rdfs:comment "A thing made or adapted for a particular purpose, especially a piece of mechanical or electronic equipment (Oxford Dictionary of English)."^^xsd:string ;
            rdfs:label "Device" .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Distance
sbeo:Distance rdf:type owl:Class ;
              rdfs:comment "Distance class is mainly used to mention the distance between two spaces(either connected or not). As the building is represented as a graph having nodes and egdes, where as edges represents the connection between two nodes, therefore, the weight of the edge is considered as the Distance. Later on, the Distance is used to calculate the evaucation routes."^^xsd:string ;
              rdfs:label "Distance"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Earthquake
sbeo:Earthquake rdf:type owl:Class ;
                rdfs:subClassOf sbeo:Incident ;
                rdfs:comment "A sudden violent shaking of the ground, typically causing great destruction, as a result of movements within the earth's crust or volcanic action(Oxford Dictionary). It may cause a sudden panic among the occupants of the building."^^xsd:string ;
                rdfs:label "Earthquake"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#EmergencyExit
sbeo:EmergencyExit rdf:type owl:Class ;
                   rdfs:subClassOf sbeo:Exit ;
                   rdfs:comment "Emerency exit is only used in case of any emergency conditions such fire, etc."^^xsd:string ;
                   rdfs:label "EmergencyExit"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Entrance
sbeo:Entrance rdf:type owl:Class ;
              rdfs:subClassOf sbeo:SpaceCharacteristic ;
              rdfs:comment "Entrance represents a space or a door that is used to enter into a building. Normally, it is also used as an exit."^^xsd:string ;
              rdfs:label "Entrance"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Exit
sbeo:Exit rdf:type owl:Class ;
          rdfs:subClassOf sbeo:SpaceCharacteristic ;
          rdfs:comment "Exit represents a space or a door that is used to exit the building. Generally, an exit is as same as an entrance."^^xsd:string ;
          rdfs:label "Exit"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Family
sbeo:Family rdf:type owl:Class ;
            rdfs:subClassOf foaf:Person ;
            rdfs:comment "A group of persons having family ties between them."^^xsd:string ;
            rdfs:label "Family"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Fire
sbeo:Fire rdf:type owl:Class ;
          rdfs:subClassOf sbeo:Incident ;
          rdfs:comment "An event that may burn different parts of the building and hazourdous for both the people and the building."^^xsd:string ;
          rdfs:label "Fire"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#FireDoor
sbeo:FireDoor rdf:type owl:Class ;
              rdfs:subClassOf sbeo:PassiveFireProtection ;
              rdfs:comment "A type of door used to minimize the spread of fire and related effects by isolating two connected spaces or sections in the building."^^xsd:string ;
              rdfs:label "FireDoor"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#FireExtinguisher
sbeo:FireExtinguisher rdf:type owl:Class ;
                      rdfs:subClassOf sbeo:ActiveFireProtection ;
                      rdfs:comment "An active fire protection instrument used by persons to blow out small scale fire."^^xsd:string ;
                      rdfs:label "FireExtinguisher"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#FireProtection
sbeo:FireProtection rdf:type owl:Class ;
                    rdfs:subClassOf sbeo:IncidentProtection ;
                    rdfs:comment "It is a practice to minimize the effects of potentially danger fires."^^xsd:string ;
                    rdfs:label "FireProtection"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Hearing
sbeo:Hearing rdf:type owl:Class ;
             rdfs:subClassOf sbeo:SensoryImpairment ;
             rdfs:comment "It is a partial or total unability to hear. It is also known as Hearing loss."^^xsd:string ;
             rdfs:label "Hearing"^^xsd:string .


###  http://www.ia.urjc.es/ontologies/building/sbeo#Impaired
sbeo:Impaired rdf:type owl:Class ;
              rdfs:subClassOf foaf:Person ;
